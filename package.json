{
  "dependencies": {
    "@nestjs/common": "7.0.3",
    "@nestjs/core": "7.0.3",
    "@nestjs/platform-express": "7.0.3",
    "@nestjs/swagger": "4.4.0",
    "@types/compression": "1.7.0",
    "@types/express": "4.17.2",
    "@types/hapi__joi": "16.0.12",
    "@types/helmet": "0.0.45",
    "@types/node": "~12",
    "@wireapp/core": "15.12.2",
    "@wireapp/lru-cache": "3.4.9",
    "body-parser": "1.19.0",
    "celebrate": "12.0.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.1",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fs-extra": "8.1.0",
    "helmet": "3.21.3",
    "http-status-codes": "1.4.0",
    "moment": "2.24.0",
    "pm2": "4.2.3",
    "pm2-logrotate": "2.7.0",
    "pure-uuid": "1.6.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "swagger-ui-express": "4.1.4",
    "typescript": "3.8.3"
  },
  "description": "End-to-end Test Service (ETS) for Wire's test automation suite.",
  "devDependencies": {
    "@types/jasmine": "3.5.9",
    "@types/swagger-ui-express": "4.1.1",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "@wireapp/eslint-config": "1.4.0",
    "@wireapp/prettier-config": "0.3.0",
    "axios": "0.19.2",
    "babel-eslint": "10.0.3",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jasmine": "4.1.0",
    "eslint-plugin-jsdoc": "21.0.0",
    "eslint-plugin-no-unsanitized": "3.0.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.4.0",
    "eslint-plugin-simple-import-sort": "5.0.1",
    "eslint-plugin-sort-keys-fix": "1.1.0",
    "husky": "4.2.3",
    "jasmine": "3.5.0",
    "lint-staged": "10.0.8",
    "logdown": "3.3.1",
    "nock": "12.0.3",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "ts-node": "8.7.0"
  },
  "files": [
    "dist",
    "swagger.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md,js,yml}": [
      "prettier --write"
    ],
    "src/**/!(*.spec).ts": [
      "tslint --project tsconfig.json --fix"
    ],
    "src/**/*.spec.ts": [
      "tslint --project tsconfig.jasmine.json --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "wire-web-ets",
  "prettier": "@wireapp/prettier-config",
  "private": true,
  "repository": "https://github.com/wireapp/wire-web-ets",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "fix": "yarn fix:code && yarn fix:other",
    "fix:code": "yarn lint:code --fix",
    "fix:other": "yarn prettier --write",
    "lint": "yarn lint:code && yarn lint:other",
    "lint:code": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:other": "yarn prettier --list-different",
    "main": "ts-node src/main.ts",
    "postversion": "git push && git push --tags",
    "prettier": "prettier \"**/*.{json,md,js,yml}\"",
    "preversion": "yarn && yarn test && yarn dist",
    "start": "pm2 start",
    "start:dev": "cross-env NODE_DEBUG=\"@wireapp/*\" ts-node src/index.ts",
    "test": "ts-node -P tsconfig.jasmine.json node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "version": "yarn dist"
  },
  "version": "1.21.0"
}
